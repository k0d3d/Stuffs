append head
  link(rel='stylesheet', href='/css/table-fixed-header.css')

block body
  include ../includes/modals/modal-order-supplied.jade
  .row-fluid.row-break
    .span2.actioncol
      button.btn.btn-large.btn-block.btn-primary(ng-click="template.url = 'templates/admin-configuration-tpl'") Settings
      button.btn.btn-large.btn-block.btn-primary(ng-click="template.url = 'templates/admin-transactions-tpl'") Stock Transactions
      button.btn.btn-large.btn-block.btn-primary(ng-click="template.url = 'templates/admin-bill-profile-tpl'") Manage Billing
    .span10
      .row(ng-include="template.url")
        .span11.offset1()
          .midheader(style="padding: 20px")
            h3 Click on the buttons on the left
            h5 and I'll go away.



append foot
  script(src="js/table-fixed-header.js")                

